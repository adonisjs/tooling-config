  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/utils": "^6.0.0"
  },
  "devDependencies": {
    "@japa/assert": "^1.4.1",
    "@japa/runner": "^2.5.1",
    "@japa/spec-reporter": "^1.3.3",
    "@types/node": "^20.4.1",
    "@typescript-eslint/parser": "^6.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
