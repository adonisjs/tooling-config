{
  "name": "@adonisjs/eslint-plugin",
  "version": "1.1.7",
  "description": "ESLint plugin to enforce AdonisJS app specific linting rules",
  "files": [
    "dist"
  ],
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
